{% extends 'base.html.twig' %}
  
      
{% block head %}    
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="../../assets/css/style.css">
  <link rel="stylesheet" href="../../node_modules/animate.css/animate.min.css">
  <link rel="stylesheet" href="../../node_modules/aos/dist/aos.css">
  <link rel="stylesheet" href="../../node_modules/bootstrap/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="../../node_modules/sweetalert2/dist/sweetalert2.min.css">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Meow+Script&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Pacifico&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Teko&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Indie+Flower&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Yaldevi:wght@200&display=swap" rel="stylesheet">
  <script src="../../node_modules/jquery/dist/jquery.min.js"></script>
  <script src="../../node_modules/aos/dist/aos.js"></script>
  <script src="../../node_modules/vue/dist/vue.js"></script>
{% block title %}QivendQiloue?{% endblock %}
{% endblock %}
      
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
<style>
h2 {
    font-family: 'Pacifico', cursive;
    color: #61b515;
}

a{
    color: #61b515 !important;
}

.slogan {
    font-family: 'Meow Script', cursive;
    font-size: 30px;
    margin-top: 15px;
    margin-left: 30px;
}

span{
    margin-top: auto;
    margin-bottom: auto;
}

.photoEntete{
    background-image: url('../img/entete.jpg');
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    height: 700px;
    width: 100%;
    box-shadow: 1px 0px 1px 1px black;
    z-index: 1;
}

.container-fluid{
    margin-left: auto;
    margin-right:auto;
    width: 100%;
}

.navbar{
    position: fixed !important;
    z-index: 2;
    background-color: white;
    width: 100%;
    box-shadow: 1px 0px 1px 1px black;
}

.row{
    margin-left: auto;
    margin-right: -50px !important;
    width: 100%;
}

.nav-item{
    padding-left: 50px;
    padding-right: 50px;
    border-right: 0.3px solid #0a3a5b !important;
    font-family: 'Meow Script', cursive; 
    font-size: 30px;
}

.card{
    border: 2px green;
    text-align: center;
}

h5{
    color: #61b515;
    font-family: 'Indie Flower', cursive;
    font-size: 30px !important;
}

.nav-item:hover{
background-color: #61b515;
}

.nav-link:hover{
    color: ivory !important;
    
transform: scale(1.2);
color: ivory !important;
}

#collapseOne{
    position: relative;
    z-index: 3;
}

p{
    font-family: 'Yaldevi', sans-serif;
}

.sousmenu {
    display: none;
    }
     
   [class*=menu]:hover{
    display:block;
    position:relative;
    z-index: 3;
    }
       
   #logo{
       z-index: 3;
       position: absolute;
       margin-top: -30px;    
   }

   #slogan{
       margin-left: 120px;
       z-index: 3;
       position: absolute;
       margin-top: -20px;
   }

   .btn{
       background-color: #0a3a5b;
   }

   .toit{
    background-color: #61b515;  
    width: 100%;
  height: 400px;
  border-left: 1272px solid transparent;
  border-right: 1272px solid transparent;
  border-bottom: 150px  solid #ffffff;                   
   }

.gouttiere{
    height: 30px;
    background-color: rgb(186, 190, 150);    
    border-left: 1270px solid transparent;
    border-right: 1270px solid transparent;
    border-bottom: 100px  solid white;      
}

   .fenetre{
    background-color: white;  
    height: 400px;
   }

   .space{
       height: 80px;
   }

   .facade{
       height: 400px;
   }
</style>
{% endblock %}
{% block javascripts %}
            {{ encore_entry_script_tags('app') }}
{% endblock %}
{% block body %}
{% block content %}

  <div class="container-fluid px-0">
    <div class="photoEntete"></div>
  </div>

  

  <div class="container">
    <div class="row ">
      <div id="card-choice" class="card-group">
        <themeCard v-for="item in processList" v-bind:todo="item" v-bind:key="item.id"></themeCard>
      </div>
    </div>
  </div>

  <div class="container">
    <div class="row ">
      <div id="card-choice-again" class="card-group">
        <themeCardAgain v-for="item in processList" v-bind:todo="item" v-bind:key="item.id"></themeCardAgain>
      </div>
    </div>
  </div>
<div class="space"></div>
 
  <div class="toit">
    <div class="row ">
      <div id="card-choice-again" class="card-group">
        
        <theme_card_again v-for="item in processList" v-bind:todo="item" v-bind:key="item.id"></theme_card_again>
      </div>
    </div>
 

  <div class="fenetre">
    <div class="row ">
      <div id="card-choice-again" class="card-group">
        <theme_card_again v-for="item in processList" v-bind:todo="item" v-bind:key="item.id"></theme_card_again>
      </div>
    </div>
  </div>
 
<div class="facade"></div>

{% endblock %}
<script>AOS.init();</script>
<script src="https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js"></script>
<script src="../../node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="../../node_modules/sweetalert2/dist/sweetalert2.min.js"></script>
<script src="../../assets/js/main.js"></script>
{% endblock %}
